<div id="group">
    <% if (totalNum > 0) { %>
        <ul class="pagination">
            <% if (pageNum == 1) { %>
                <li class="page-item disabled"><a class="page-link">First</a></li>
            <% } else { %>
                <li class="page-item"><a class="page-link" href="<%=url+"?pageNum=1"%>">First</a></li>
            <% } %>
            <% var i = (Number(pageNum) > 5 ? Number(pageNum) - 4 : 1) %>
            <% if (i !== 1) { %>
                <li class="page-item disabled"><a class="page-link">...</a></li>
            <% } %>
            <% for (; i <= (Number(pageNum) + 4) && i <= totalNum; i++) { %>
                <% if (i == pageNum) { %>
                    <li class="page-item active"><a class="page-link"><%= i %></a></li>
                <% } else { %>
                    <li class="page-item"><a class="page-link" href="<%=url +"?pageNum="+i %>"><%= i %></a></li>
                <% } %>
                <% if (i == Number(pageNum) + 4 && i < totalNum) { %>
                    <li class="page-item disabled"><a class="page-link">...</a></li>
                <% } %>
            <% } %>
            <% if (pageNum == totalNum) { %>
                <li class="page-item disabled"><a class="page-link">Last</a></li>
            <% } else { %>
                <li class="page-item"><a class="page-link" href="<%=url+"?pageNum="+ totalNum %>">Last</a></li>
            <% } %>
        </ul>
    <% } %>
</div>

